    android:layout_height="match_parent"
    tools:context="com.example.yehezkiel.eclassapp.ThreeFragment">
    
    <android.support.v4.widget.SwipeRefreshLayout
        android:id="@+id/swiperefresh"
        android:layout_width="match_parent"
